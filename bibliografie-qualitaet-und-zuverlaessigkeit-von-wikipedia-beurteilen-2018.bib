@article{doutreix_fausse_2018,
  title =	 {La fausse information au regard des vertus
                  épistémiques de Wikipédia},
  issn =	 {1764-2507},
  url =
                  {http://www.cairn.info/revue-le-temps-des-medias-2018-1-page-91.htm?1=1&DocId=493204&hits=10+},
  doi =		 {10.3917/tdm.030.0091},
  abstract =	 {Comment la fausse information est-elle pensée et
                  impensée dans Wikipédia ? Quand le faux devient-il
                  sujet de discussions dans un contexte éditorial où
                  la vérité n’est pas censée faire l’objet de débats ?
                  Sur le cas de l’entrée « Aéroport du Grand Ouest »
                  (cf. : Notre-Dame-des-Landes), nous montrerons que
                  la fausse information est thématisée dans Wikipédia
                  comme un manquement à ses propres règles et
                  principes concernant la vérifiabilité et la
                  neutralité, plus que comme une erreur factuelle., Is
                  false information approached in Wikipedia as an
                  epistemological concern ? When and how does
                  falseness become subject to discussion in an
                  editorial context where the truth is not supposed to
                  be debated ? Taking the case of the entry “Aéroport
                  du grand ouest” (cf : Notre-Dame-des-Landes) I will
                  demonstrate that false information is identified in
                  Wikipedia as a breach of its rules and principles
                  regarding verifiability and neutrality more than a
                  factual error.},
  pages =	 {91--104},
  number =	 30,
  journaltitle = {Le Temps des médias},
  author =	 {Doutreix, Marie-Noëlle},
  urldate =	 {2018-10-04},
  date =	 {2018-03-19},
  langid =	 {french}
}

